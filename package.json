		"pinia": "^3.0.3",
		"pinia-plugin-persistedstate": "^4.3.0",
		"vue": "^3.5.16",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/create-config": "^1.9.0",
		"@eslint/js": "^9.28.0",
		"@types/node": "^24.0.1",
		"@types/nprogress": "^0.2.3",
		"@vitejs/plugin-vue": "^5.2.4",
		"@vitejs/plugin-vue-jsx": "^4.2.0",
